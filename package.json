{
  "name": "@verre/core",
  "version": "1.1.1",
  "type": "module",
  "description": "Template project for writing node package with napi-rs",
  "main": "index.js",
  "types": "index.d.ts",
  "repository": {
    "url": "git+ssh://git@github.com/napi-rs/package-template-pnpm.git",
    "type": "git"
  },
  "license": "MIT",
  "keywords": [
    "napi-rs",
    "NAPI",
    "N-API",
    "Rust",
    "node-addon",
    "node-addon-api"
  ],
  "files": [
    "index.d.ts",
    "index.js"
  ],
  "napi": {
    "binaryName": "verre_core",
    "targets": [
      "x86_64-apple-darwin",
      "aarch64-apple-darwin",
      "x86_64-pc-windows-msvc",
      "aarch64-pc-windows-msvc",
      "x86_64-unknown-linux-gnu",
      "x86_64-unknown-linux-musl",
      "aarch64-unknown-linux-gnu",
      "aarch64-unknown-linux-musl",
      "x86_64-unknown-freebsd"
    ]
  },
  "engines": {
    "node": ">= 20"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "scripts": {
    "artifacts": "napi artifacts",
    "build": "napi build --esm --platform --release",
    "build:debug": "napi --esm build --platform",
    "format": "run-p format:rs",
    "format:rs": "cargo fmt",
    "lint": "oxlint",
    "prepublishOnly": "napi prepublish -t npm",
    "test": "vitest run",
    "version": "napi version"
  },
  "devDependencies": {
    "@napi-rs/cli": "^3.2.0",
    "@types/node": "^24.5.2",
    "npm-run-all2": "^8.0.4",
    "oxlint": "^1.14.0",
    "typescript": "^5.9.2",
    "uWebSockets.js": "github:uNetworking/uWebSockets.js#v20.52.0",
    "vitest": "^3.2.4"
  },
  "packageManager": "pnpm@10.17.0"
}
